{
  "manifest_version": 3,
  "name": "Deepak",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": [
    "https://google.com/",
    "https://diksha.gov.in/",
    "https://deepak.prashne.com"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
